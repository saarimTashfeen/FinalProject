<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/artistNameList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="testTextView"
        />

    <TextView
        android:id="@+id/dashList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:layout_toRightOf="@+id/artistNameList"
        />

    <TextView
        android:id="@+id/songNameList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/dashList"
        />


    <TextView
        android:id="@+id/listID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/songNameList"
        />


</RelativeLayout>